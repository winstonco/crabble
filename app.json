{
  "expo": {
    "name": "crabble",
    "slug": "crabble",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "name": "Crabble",
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    }
  },
  "overrides": {
    "metro": "0.76.0",
    "metro-resolver": "0.76.0"
  }
}
